<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proyecto_Final.MainPage"
             xmlns:icons="clr-namespace:Proyecto_Final.Fuentes;assembly=Proyecto_Final">
    
              

    <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
        <Label Text="pedorra" FontFamily="letra"></Label>
        <Label FontFamily="fontello" Text="&#xE800;"></Label>
        <Label FontFamily="fontello" Text="{x:Static icons:Icons.LockOpenAlt}"></Label>
